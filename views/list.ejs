<%- include header %>

    <div class="container">
      <table class="table">
        <thead>
          <tr>
            <th>#</th>
            <th>图书名称</th>
            <th>图书价格</th>
            <th>详情</th>
            <th>操作</th>
          </tr>
          <% for(var i = 0; i< row.length; ++ i) { %>
          <tr>
            <% s1 = "/list/" + i.toString() %>
            <% s2 = "/list/add/" + i.toString() %>
            <td><%= i %></td>
            <td><%= row[i].bookname %></td>
            <td><%= row[i].price %></td>
            <td><a href=<%= s1 %>>详情</a></td>
            <td><a href=<%= s2 %>>放入购物车</a></td>
          </tr>
          <% } %>
        </thead>
      </table>
    </div>

<%- include footer %>




